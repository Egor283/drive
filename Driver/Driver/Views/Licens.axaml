<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Driver.Views.Licens">
    <Grid RowDefinitions="*, 50">
        <DataGrid Name="DG" AutoGenerateColumns="True" Grid.Row="0" IsReadOnly="True"/>
        <StackPanel Orientation="Horizontal" Grid.Row="1">
        <Button Name="NazBtn" Click="NazBtn_OnClick" Content="Назад"/>
        <Button Content="Посмотреть историю ВУ" Name="HIsBtn" Click="HIsBtn_OnClick" />
        <Button Content="Генерировать лицензию" Name="GnBtn" Click="GnBtn_OnClick"/>
        </StackPanel>
    </Grid>
</UserControl>
